    || "CLK_BUS_PERIPHERAL_3"
    || "CLK_BUS_PERIPHERAL_4"
    || "CLK_BUS_PERIPHERAL_5"
    || "CLK_BUS_PERIPHERAL_6"
    || "CLK_BUS_PERIPHERAL_7"
    || "CLK_BUS_PERIPHERAL_8"
    
enum  CLK_BUSES_REFERENCE
    "CLK_BUS_REFERENCE_1"
    || "CLK_BUS_REFERENCE_2"
    || "CLK_BUS_REFERENCE_3"
    || "CLK_BUS_REFERENCE_4"
    || "CLK_BUS_REFERENCE_5"

config USE_SYS_CLK
    bool "Use Clock System Service?"
    default y
    ---help---
    IDH_HTML_Oscillator_System_Service_Library
    ---endhelp---

config SYS_CLK_MANAGER_PLUGIN_SELECT
	string
	depends on USE_SYS_CLK
	default "mx1:MX1ClockModel" if DS60001168 || DS60001185 || DS60001290
    default "mx2:MX2ClockModel" if DS60001143 || DS60001156
    default "mz:MZClockModel"  if DS60001191 || DS60001320
    default "mzda:MZDAClockModel" if DS60001361
    default "mk:MKClockModel" if DS60001402
	default "wk:WKClockModel" if PIC32WK

config USE_SYS_CLK_DYNAMIC_NEEDED
	depends on USE_SYS_CLK
    bool
    default n

config USE_SYS_CLK_UPLL_NEEDED
	depends on USE_SYS_CLK
    bool
    default n

config SYS_CLK_MODE
    string "Select Service Mode"
    default "DYNAMIC" if USE_SYS_CLK_DYNAMIC_NEEDED
    default "STATIC"
    depends on USE_SYS_CLK
    range CLK_MODES
    ---help---
    IDH_HTML_SYSTEM_CLK_Introduction
    ---endhelp---

execute CLOCK_EXEC
	depends on USE_SYS_CLK
    prompt "Launch Clock Configurator"
    default "clock"
    ---help---
    IDH_HTML_CONFIGURATOR_Configuring_the_Oscillator_Module_Using_the_MHC_Clock_Configurator
    ---endhelp---

menu "Clock Configurator Settings"
	depends on USE_SYS_CLK
    ---help---
    IDH_HTML_CONFIGURATOR_Configuring_the_Oscillator_Module_Using_the_MHC_Clock_Configurator
    ---endhelp---

comment "**** All settings listed here can be configured using the Clock Configurator ****"
	depends on USE_SYS_CLK
    ---help---
    IDH_HTML_CONFIGURATOR_Configuring_the_Oscillator_Module_Using_the_MHC_Clock_Configurator
    ---endhelp---
    
config SYS_CLK_FSOSCEN_OVERRIDE
    bool
    depends on USE_SYS_CLK
    default n

config SYS_CLK_FRCDIV
	string
	depends on USE_SYS_CLK
	default "OSC_FRC_DIV_2" if PIC32MX
	default "OSC_FRC_DIV_1" if PIC32MZ || PIC32MK
	default "OSC_FRC_DIV_1" if PIC32WK

config SYS_CLK_PBCLK0_ENABLE
    bool
    depends on USE_SYS_CLK
    default y

config SYS_CLK_PBDIV0_OVERRIDE
    bool "Set PBDIV (Override FPBDIV)"
    depends on USE_SYS_CLK && PIC32MX
    default n
    ---help---
	IDH_HTML_CONFIGURATOR_Configuring_the_Peripheral_Bus_Clocks_MX
    ---endhelp---
    
config SYS_CLK_PBDIV0_MX
    string "Peripheral Clock Bus Divisor (1-8)" if PIC32MX
    depends on USE_SYS_CLK && PIC32MX
    persistent if SYS_CLK_PBDIV0_OVERRIDE = n
    default "DIV_1"
    range CLK_PBDIV_MX
    ---help---
	IDH_HTML_CONFIGURATOR_Configuring_the_Peripheral_Bus_Clocks_MX
    ---endhelp---

config SYS_CLK_PBDIV0_MZ
    int "Peripheral Clock Bus #1 Divisor (1-128)" if PIC32MZ || PIC32MK || PIC32WK
    depends on USE_SYS_CLK && (PIC32MZ || PIC32MK || PIC32WK)
    default 2 if !DS60001402
    default 1 if DS60001402
    range 1 128
    ---help---
	IDH_HTML_CONFIGURATOR_Configuring_the_Peripheral_Bus_Clocks_MZ
    ---endhelp---

config SYS_CLK_PBDIV0
    int
    depends on USE_SYS_CLK
    default 1 if PIC32MX && SYS_CLK_PBDIV0_MX = "DIV_1"
    default 2 if PIC32MX && SYS_CLK_PBDIV0_MX = "DIV_2"
    default 4 if PIC32MX && SYS_CLK_PBDIV0_MX = "DIV_4"
    default 8 if PIC32MX && SYS_CLK_PBDIV0_MX = "DIV_8"
    default SYS_CLK_PBDIV0_MZ if (PIC32MZ || PIC32MK || PIC32WK)
    
ifblock PIC32WK = y

config SYS_CLK_NOSC
	string "System Clock Source Configuration"
	depends on USE_SYS_CLK
	range OSC_SYS_TYPE
	default "OSC_FRC_WITH_PLL"

menu "System PLL Configuration"
	depends on USE_SYS_CLK

config SYS_CLK_SBYPASS
    bool "ByPass System PLL"
    default n
    depends on USE_SYS_CLK

config SYS_CLK_SPLLICLK
	string "System PLL Input Clock Selection"
	depends on USE_SYS_CLK
	range OSC_SYSPLL_IN_CLK_SOURCE
	default "OSC_SYSPLL_IN_CLK_SOURCE_FRC"

config SYS_CLK_SPLLIDIV
	int "System PLL Input Clock Divider (1-32)"
	depends on USE_SYS_CLK
	range 1 32
	default 1

config SYS_CLK_SPLLMULT
	int "System PLL Multiplier (1-256)"
	depends on USE_SYS_CLK
	default 128
	range 1 256

config SYS_CLK_SPLLODIV
	string "System PLL Output Clock Divider"
	depends on USE_SYS_CLK
