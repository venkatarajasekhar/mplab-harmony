    || "Peripheral Clock PBCLK"
    || "Primary Oscillator POSC"
    || "Internal Fast Oscillator FRC"
    || "Low-Power Oscillator LPRC"
    || "Secondary Oscillator SOSC"
    || "USB PLL UPLL"
    || "System PLL output SPLL"
    || "Reference Clock Input REFCLKI"

enum CLK_REFCLK_SOURCE_MZ
    "System Clock SYSCLK"
    || "Peripheral Clock #1 PBCLK1"
    || "Primary Oscillator POSC"
    || "Internal Fast Oscillator FRC"
    || "Low-Power Oscillator LPRC"
    || "Secondary Oscillator SOSC"
    || "System PLL output SPLL"
    || "Reference Clock Input REFCLKIx"
    || "Internal Backup FRC BFRC"

enum CLK_REFCLK_SOURCE_MK
    "System Clock SYSCLK"
    || "Peripheral Clock #1 PBCLK1"
    || "Primary Oscillator POSC"
    || "Internal Fast Oscillator FRC"
    || "Low-Power Oscillator LPRC"
    || "Secondary Oscillator SOSC"
    || "USB Clock USBCLK"
    || "System PLL output SPLL"
    || "Reference Clock Input REFCLKIx"

enum CLK_REFCLK_SOURCE_WK
    "System Clock SYSCLK"
    || "Peripheral Clock #1 PBCLK1"
