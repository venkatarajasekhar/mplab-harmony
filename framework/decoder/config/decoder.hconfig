menu "Decoder"

menu "Decoder"

menu "Image Decoders"

enum JPEG_DATA_BUF_LEN_ENUM
	"128" ||
	"256" ||
	"512" ||
	"1024" ||
	"2048" ||
	"4096" ||
	"10240" ||
	"20240" ||
	"40480" ||
	"51200"

config USE_IMAGE_DECODER_NEEDED
    bool
    default n

config USE_IMAGE_DECODER
    bool "Use Image Decoder Libraries?"
    default y if USE_IMAGE_DECODER_NEEDED
    default n

comment "******* WARNING: Using Image Decoder Library requires the using Graphics Library *******"
depends on USE_IMAGE_DECODER && !USE_GFX_STACK

config USE_JPEG_DECODER_NEEDED
bool
default n
depends on USE_IMAGE_DECODER

config USE_JPEG_DECODER
bool "Use JPEG Decoder?"
depends on USE_IMAGE_DECODER
default y if USE_JPEG_DECODER_NEEDED
default n

config JPEG_DATA_BUF_LEN
string "JPEG Decoder Burst Buffer Length (bytes)"
depends on USE_JPEG_DECODER
default "512"
range JPEG_DATA_BUF_LEN_ENUM

config USE_BMP_DECODER_NEEDED
bool
default n
depends on USE_IMAGE_DECODER

config USE_BMP_DECODER
bool "Use BMP Decoder?"
depends on USE_IMAGE_DECODER
default y if USE_BMP_DECODER_NEEDED
default n

config USE_GIF_DECODER_NEEDED
bool
default n
depends on USE_IMAGE_DECODER

config USE_GIF_DECODER
bool "Use GIF Decoder?"
depends on USE_IMAGE_DECODER
default y if USE_GIF_DECODER_NEEDED
default n

endmenu

menu "Audio Decoders"

config USE_DECODER
bool "Use Audio Decoder Libraries?"
default n

    config USE_WMA_DECODER
    bool "Use WMA Decoder Library (premium purchase)"
    default n
    depends on USE_DECODER
        config WMA_DECODER_LIB_SELECT
        string "WMA Library"
        depends on USE_DECODER && USE_WMA_DECODER && PIC32MZ
        range WMA_LIB_TARGET
        default "microAptiv Library"

    config USE_MP3_DECODER
    bool "Use MP3 Decoder Library (premium purchase)"
    default n
    depends on USE_DECODER
        config MP3_DECODER_LIB_SELECT
        string "MP3 Library"
        depends on USE_DECODER && USE_MP3_DECODER && PIC32MZ
        range MP3_DECODER_LIB
        default "Advanced Library (MPEG 1/2/2.5)"

    config USE_AAC_DECODER
    bool "Use AAC Decoder Library (premium purchase)"
    default n
    depends on USE_DECODER
        config AAC_DECODER_LIB_SELECT
        string "AAC Library"
        depends on USE_DECODER && USE_AAC_DECODER && PIC32MZ
        range DECODER_LIB_TARGET
        default "microAptiv Library"

    config USE_OGG_SPEEX_DECODER
    bool "Use Ogg-Speex Decoder Library"
    default n
    depends on USE_DECODER
	
	config USE_OGG_OPUS_DECODER
	bool "Use Ogg-Opus Decoder Library"
	default n
	depends on USE_DECODER

    config USE_WAV_STREAMING
    bool "Use WAV AUDIO stream"
    default n
    depends on USE_DECODER

    config USE_ADPCM_STREAMING
    bool "Use ADPCM AUDIO stream"
    default n
    depends on USE_DECODER
	
endmenu

